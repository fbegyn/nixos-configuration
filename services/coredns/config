(forward-cf) {
    forward . tls://1.1.1.1 tls://1.0.0.1 {
        tls_servername cloudflare-dns.com
        health_check 5s
    }
    log
    errors
    cache
}

(forward-opennic) {
    forward . 185.121.177.177 169.239.202.202
    log
    errors
    cache
}

. {
    prometheus 10.3.2.2:9153

    auto begyn.lan {
        directory /etc/coredns/zones
    }

    hosts /etc/coredns/hosts/ads-fakenews {
        fallthrough
    }

    import forward-cf
}

